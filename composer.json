{
    "name": "jobapis/jobs-common",
    "type": "library",
    "description": "Making it simple to integrate your application with job api providers.",
    "keywords": [
        "job boards",
        "jobs",
        "api client",
        "object"
    ],
    "homepage": "http://jobapis.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "http://www.karllhughes.com"
        },
        {
            "name": "Steven Maguire",
            "email": "stevenmaguire@gmail.com",
            "homepage": "https://github.com/stevenmaguire"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.6",
        "phpunit/php-code-coverage": "~2.0",
        "mockery/mockery": ">=0.9.4",
        "dunglas/php-schema": "0.4.*",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JobApis\\Jobs\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobApis\\Jobs\\Client\\Tests\\": "tests/src/",
            "JobApis\\Jobs\\Client\\Fixtures\\": "tests/fixtures/"
        }
    }
}
